  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/start"
    },
    {
      "name": "Launch index.html",
      "type": "chrome",
      "request": "launch",
      "runtimeExecutable": "/usr/lib/chrome",
      "url": "http://localhost:3000",
      "file": "${workspaceFolder}/src/index.js"
    }
  ]
}